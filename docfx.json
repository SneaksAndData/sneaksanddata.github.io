{
  "build": {
    "markdownEngineProperties": {
      "markdigExtensions": [
        "attributes"
      ]
    },
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/material"
    ],
    "globalMetadata": {
      "_appName": "Ecco Sneaks\u0026Data",
      "_appTitle": "Ecco Sneaks\u0026Data",
      "_appLogoPath": "images/code.svg",
      "_appFaviconPath": "images/code.svg",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
